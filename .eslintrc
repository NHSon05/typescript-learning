{
    "extends": ["react-app", "prettier"],
    "plugins": ["react","prettier"],
    "rules": {
        "prettier/prettier": [
            "warning", {    
                "arrowParenfalses": "always",
                "semi": false,       
                "trailingComma": "none",
                "tabWidth": 2,
                "endOfLine": "auto",
                "useTabs": false,
                "singleQuote": true,
                "printWidth": 120,
                "jsxSingleQuote": true  
            }
        ]
    }
}